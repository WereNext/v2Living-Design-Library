            "@mui/material": "^7.3.6",
            "@radix-ui/react-accordion": "^1.2.3",
            "@radix-ui/react-alert-dialog": "^1.1.6",
            "@radix-ui/react-aspect-ratio": "^1.1.2",
            "@radix-ui/react-avatar": "^1.1.3",
            "@radix-ui/react-checkbox": "^1.1.4",
            "@radix-ui/react-collapsible": "^1.1.3",
            "@radix-ui/react-context-menu": "^2.2.6",
            "@radix-ui/react-dialog": "^1.1.6",
            "@radix-ui/react-dropdown-menu": "^2.1.6",
            "@radix-ui/react-hover-card": "^1.1.6",
            "@radix-ui/react-label": "^2.1.2",
            "@radix-ui/react-menubar": "^1.1.6",
            "@radix-ui/react-navigation-menu": "^1.2.5",
            "@radix-ui/react-popover": "^1.1.6",
            "@radix-ui/react-progress": "^1.1.2",
            "@radix-ui/react-radio-group": "^1.2.3",
            "@radix-ui/react-scroll-area": "*",
            "@radix-ui/react-select": "^2.1.6",
            "@radix-ui/react-separator": "^1.1.2",
            "@radix-ui/react-slider": "^1.2.3",
            "@radix-ui/react-slot": "^1.1.2",
            "@radix-ui/react-switch": "^1.1.3",
            "@radix-ui/react-tabs": "^1.1.3",
            "@radix-ui/react-toggle": "^1.1.2",
            "@radix-ui/react-toggle-group": "^1.1.2",
            "@radix-ui/react-tooltip": "^1.1.8",
            "@supabase/supabase-js": "^2.89.0",
            "@testing-library/jest-dom": "*",
            "@testing-library/react": "*",
            "@vitejs/plugin-react": "*",
            "antd": "^6.1.4",
            "bootstrap": "^5.3.8",
            "class-variance-authority": "^0.7.1",
            "clsx": "*",
            "cmdk": "^1.1.1",
            "embla-carousel-react": "^8.6.0",
            "framer-motion": "^12.24.0",
            "fs": "*",
            "hono": "^4.11.5",
            "input-otp": "^1.4.2",
            "lucide-react": "^0.487.0",
            "next-themes": "^0.4.6",
            "path": "*",
            "react": "^18.3.1",
            "react-bootstrap": "^2.10.10",
            "react-day-picker": "^8.10.1",
            "react-dom": "^18.3.1",
            "react-hook-form": "^7.55.0",
            "react-resizable-panels": "^2.1.7",
            "react-syntax-highlighter": "*",
            "recharts": "^2.15.2",
            "sonner": "^2.0.3",
            "tailwind-merge": "*",
            "url": "*",
            "vaul": "^1.1.2",
            "vitest": "*"
      },
      "devDependencies": {
            "@types/jsdom": "^27.0.0",
            "@types/node": "^20.10.0",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "concurrently": "^9.2.1",
            "electron": "^40.0.0",
            "electron-builder": "^26.4.0",
            "jsdom": "^27.4.0",
            "typescript": "^5.9.3",
            "vite": "6.3.5",
            "wait-on": "^9.0.3"
      },
      "scripts": {
            "dev": "concurrently \"vite\" \"npm:mcp:http\" --names \"APP,MCP\" --prefix-colors \"cyan,magenta\"",
            "dev:app-only": "vite",
            "dev:full": "concurrently \"npm:dev:app-only\" \"npm:mcp:http\" --names \"APP,MCP-HTTP\" --prefix-colors \"cyan,magenta\"",
            "dev:with-stdio": "concurrently \"npm:dev\" \"npm:mcp:start\" --names \"APP,MCP-STDIO\" --prefix-colors \"cyan,blue\"",
            "dev:with-mcp": "./start-with-mcp.sh",
            "mcp:start": "node src/mcp-server/index.js",
            "mcp:http": "node src/mcp-server/http-server.js",
            "mcp:dev": "node --watch src/mcp-server/index.js",
            "mcp:http-dev": "node --watch src/mcp-server/http-server.js",
            "build": "vite build",
            "test": "vitest",
            "electron:dev": "concurrently \"npm:dev\" \"wait-on http://localhost:3000 && electron .\"",
            "electron:build": "npm run build && electron-builder",
            "electron:start": "electron ."
      },
      "build": {
            "appId": "com.livingdesignlibrary.app",
            "productName": "Living Design Library",
            "directories": {
                  "output": "dist-electron"
            },
            "files": [
                  "build/**/*",
                  "electron/**/*"
            ],
            "extraResources": [
                  {
                        "from": "src/mcp-server",
                        "to": "mcp-server",
                        "filter": [
                              "**/*.js",
                              "**/*.json",
                              "ui-apps/**/*",
                              "!node_modules/**/*"
                        ]
                  }
            ],
            "mac": {
                  "category": "public.app-category.developer-tools",
                  "target": ["dmg", "zip"]
            },
            "win": {
                  "target": ["nsis", "portable"]
            },
            "linux": {
                  "target": ["AppImage", "deb"]
            }
      }
}
